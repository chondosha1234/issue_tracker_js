<%- include("partials/base"); %>

<link rel="stylesheet" href="/stylesheets/data_view.css" />

<div class="body-container">

  <div class="main-container">
    <h2> User List </h2>
    <br>
    <ul class ="user-list">
      <% for (let i = 0; i < users.length; i++){ %>
      <li class="user-list-item">
        <p>
          <a class="user-info-link" href="/home/users/user_info/<%= users[i].username %>">
            <%= users[i].username %>
          </a>
        </p>
        <p class="user-date"> Created On: <%= users[i].created_on.toISOString().slice(0, 10).replace('T', ' ') %> </p>
      </li>
      <% } %>
    </ul>
  </div>

  <%- include("partials/sidebar"); %>

</div>
